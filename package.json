{
	"name": "fuzzytable",
	"version": "0.0.46",
	"author": "Jon Bitgood <jon@bitgood.cloud>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonbitgood/fuzzytable.git"
	},
	"license": "MIT",
	"keywords": [
		"svelte",
		"datatable",
		"fuzzysearch"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.23.0",
		"@sveltejs/package": "^2.2.1",
		"autoprefixer": "^10.4.15",
		"postcss": "^8.4.29",
		"publint": "^0.1.16",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"fuse.js": "^6.6.2"
	}
}
