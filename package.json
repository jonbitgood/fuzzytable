{
  "name": "fuzzy-table",
  "type": "module",
  "version": "0.1.0",
  "description": "A JavaScript module to render and manage a dynamic data table with sorting, filtering, and pagination.",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
