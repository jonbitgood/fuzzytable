  },
  "keywords": [
    "data-table",
    "filter",
    "sort",
    "pagination"
  ],
  "author": "Jon Bitgood",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2"
  }
